{
    "name": "echoes-ai",
    "version": "0.1.0",
    "private": true,
    "workspaces": [
        "frontend",
        "backend"
    ],
    "scripts": {
        "front:dev": "pnpm --filter frontend dev",
        "front:build": "pnpm --filter frontend build",
        "front:start": "pnpm --filter frontend start",
        "back:dev": "pnpm --filter backend dev",
        "back:build": "pnpm --filter backend build",
        "back:start": "pnpm --filter backend start",
        "dev": "pnpm run --parallel front:dev back:dev"
    }
}