{
    "name": "echoes-ai",
    "version": "0.1.0",
    "private": true,
    "workspaces": [
        "frontend",
        "backend"
    ],
    "scripts": {
        "front:dev": "pnpm --filter echoes-ai-frontend dev",
        "front:build": "pnpm --filter echoes-ai-frontend build",
        "front:start": "pnpm --filter echoes-ai-frontend start",
        "back:dev": "pnpm --filter echoes-ai-backend dev",
        "back:build": "pnpm --filter echoes-ai-backend build",
        "back:start": "pnpm --filter echoes-ai-backend start",
        "dev": "pnpm run --parallel front:dev back:dev"
    }
}